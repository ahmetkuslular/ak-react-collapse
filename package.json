{
  "name": "ak-react-collapse",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ahmetkuslular.github.io/ak-react-collapse",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-is": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2",
    "velocity-react": "^1.4.3"
  },
  "scripts": {
    "start": "NODE_PATH=src:src/components react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_PATH=src:src/components jest --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "predeploy": "NODE_PATH=src:src/components yarn build",
    "deploy": "NODE_PATH=src:src/components gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
